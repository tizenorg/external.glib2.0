#, c-format
msgid "No bookmark found for URI '%s'"
msgstr "Није пронађен обележивач ка „%s“"

#: ../glib/gbookmarkfile.c:2413
#, c-format
msgid "No MIME type defined in the bookmark for URI '%s'"
msgstr "У обележивачу ка „%s“ није одређена МИМЕ врста"

