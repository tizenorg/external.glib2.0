msgid "Can't do a raw read in g_io_channel_read_line_string"
msgstr "Nevar izpildīt jēllasīšanu iekš g_io_channel_read_line_string"

#: glib/giochannel.c:1800 glib/giochannel.c:2058 glib/giochannel.c:2145
